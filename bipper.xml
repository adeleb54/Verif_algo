<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.

chan presse;
clock x;</declaration>
	<template>
		<name x="5" y="5">Bipeur</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="8" y="68">
			<name x="-25" y="85">desactive</name>
		</location>
		<location id="id1" x="-144" y="-85">
			<name x="-195" y="-119">silencieux</name>
			<label kind="invariant" x="-170" y="-68">x&lt;=10</label>
		</location>
		<location id="id2" x="144" y="-85">
			<name x="134" y="-119">sonne</name>
			<label kind="invariant" x="134" y="-68">x&lt;=4</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-68" y="-25">presse?</label>
			<nail x="-238" y="-85"/>
			<nail x="-238" y="68"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="51" y="-161">x==4</label>
			<label kind="assignment" x="34" y="-144">x:=0</label>
			<nail x="0" y="-178"/>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="85" y="-17">presse?</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="-25" y="-102">x==10</label>
			<label kind="assignment" x="-17" y="-85">x:=0</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-170" y="68">presse?</label>
			<label kind="assignment" x="-85" y="-42">x:=0</label>
		</transition>
	</template>
	<template>
		<name>Bouton</name>
		<location id="id3" x="-110" y="-25">
			<name x="-170" y="-59">desactive</name>
		</location>
		<location id="id4" x="34" y="-25">
			<name x="24" y="-59">active</name>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-68" y="17">presse!</label>
			<nail x="-34" y="17"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="-59" y="-93">presse!</label>
			<nail x="-51" y="-68"/>
		</transition>
	</template>
	<system>system Bipeur, Bouton;
    </system>
	<queries>
		<query>
			<formula>A&lt;&gt; Bouton.active or Bouton.desactive</formula>
			<comment></comment>
		</query>
		<query>
			<formula>A&lt;&gt; Bipeur.silencieux</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E&lt;&gt; Bipeur.sonne</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E&lt;&gt; Bipeur.sonne and Bipeur.silencieux</formula>
			<comment></comment>
		</query>
	</queries>
</nta>
